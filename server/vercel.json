{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/blogs.*",
      "dest": "/server.js"
    },
    {
      "src": "/api/ai.*",
      "dest": "/server.js"
    },{
      "src": "/api/.*",
      "dest": "/server.js"
    }
  ]
}
